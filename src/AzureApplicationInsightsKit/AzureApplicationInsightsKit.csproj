<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Boris Zaikin</Authors>
    <Product>Azure ApplicationInsights Kit</Product>
    <Copyright>Copyright (c) Boris Zaikin</Copyright>
    <Description>Library (SDK) provides capabilities to retrieve and query Metrics, Events data from Azure Application Insights.</Description>
    <PackageProjectUrl>https://github.com/Boriszn/AzureApplicationInsightsKit</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/Boriszn/AzureApplicationInsightsKit/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/Boriszn/AzureApplicationInsightsKit</RepositoryUrl>
    <RepositoryType>GIT</RepositoryType>
    <PackageTags>azure, azure-application-insights , dot-net-core, app-insights, sdk, csharp</PackageTags>
    <PackageIconUrl>https://raw.githubusercontent.com/Boriszn/AzureApplicationInsightsKit/develop/assets/img/logo.jpg</PackageIconUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="11.0.2" />
  </ItemGroup>

</Project>
